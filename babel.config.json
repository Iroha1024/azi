{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@vue/babel-plugin-jsx",
      {
        "mergeProps": false,
        "enableObjectSlots": false
      }
    ],
    ["./plugin/babel-auto-bind-style"]
  ]
}
