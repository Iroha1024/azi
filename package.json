{
  "name": "azi",
  "version": "0.1.0",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite site",
    "format": "prettier --write \"src/**/*\" \"site/**/*\"",
    "lint": "eslint \"src/**/*\" \"site/**/*\"",
    "lint:fix": "eslint \"src/**/*\" \"site/**/*\" --fix",
    "clean": "rimraf es",
    "build": "npm run clean && babel --extensions .ts,.tsx src -d es && tsc -p src/tsconfig.json && npm run build:css",
    "build:css": "cross-env NODE_ENV=production node script/build-css",
    "clean:site": "rimraf dist",
    "build:site": "npm run clean:site && vite build site"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/types": "^7.16.8",
    "@icon-park/vue-next": "^1.3.6",
    "@types/node": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "autoprefixer": "^10.4.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.45.2",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue": "^3.2.26",
    "vue-router": "4"
  },
  "peerDependencies": {
    "vue": "^3.2.26"
  },
  "dependencies": {
    "@vueuse/core": "^7.4.1",
    "classnames": "^2.3.1",
    "vue-types": "^4.1.1"
  }
}
