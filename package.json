{
  "name": "azi",
  "version": "0.1.0",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "scripts": {
    "dev": "vite site",
    "format": "prettier --write \"src/**/*.{vue,js,ts,jsx,tsx}\" \"site/**/*.{vue,js,ts,jsx,tsx}\"",
    "clean": "rimraf es",
    "build": "npm run clean && babel --extensions .ts,.tsx src -d es && tsc -p src/tsconfig.json && npm run build:css",
    "build:css": "cross-env NODE_ENV=production node script/build-css",
    "clean:site": "rimraf dist",
    "build:site": "npm run clean:site && vite build site"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@types/node": "^17.0.4",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue": "^3.2.26"
  },
  "peerDependencies": {
    "vue": "^3.2.26"
  },
  "dependencies": {
    "vue-types": "^4.1.1"
  }
}
